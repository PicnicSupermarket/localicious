<?xml version="1.0" encoding="utf-8"?>
<resources>
  {{# SINGULAR}}
  <string name="{{ key }}">"{{{ value }}}"</string>
  {{/ SINGULAR}}
  {{# PLURAL}}
  <plurals name="{{ key }}">
  {{# value}}
    <item quantity="{{lowerCase quantity}}">"{{{ value }}}"</item>
  {{/ value}}
  </plurals>
  {{/ PLURAL}}
</resources>
